<?xml version="1.0"?>
<extension key="theme_customization" type="module">
  <file>theme_customization</file>
  <name>theme_customization</name>
  <description>To add padding for CiviCRM menu bar</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>may</author>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">http://FIXME</url>
    <url desc="Documentation">http://FIXME</url>
    <url desc="Support">http://FIXME</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2024-07-16</releaseDate>
  <version>1.0</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.72</ver>
  </compatibility>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/ThemeCustomization</namespace>
    <format>23.02.1</format>
    <angularModule>crmThemeCustomization</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty@1.0.0</mixin>
  </mixins>
</extension>
